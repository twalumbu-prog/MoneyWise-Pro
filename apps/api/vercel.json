{
    "version": 2,
    "outputDirectory": ".",
    "functions": {
        "src/index.ts": {
            "runtime": "@vercel/node",
            "includeFiles": "src/**"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "src/index.ts"
        }
    ]
}